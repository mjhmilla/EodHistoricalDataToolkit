    "screen_template":
    {
        "filter":
        {
            "GicSubIndustry":
            {
                "folder": "fundamentalData",
                "field":
                [
                    "General",
                    "GicSubIndustry"
                ],
                "isDateSeries": false,
                "condition": "==",
                "operator": "||",
                "valueType": "string",
                "values":
                [
                    "MISSING_VALUE"
                ]
            },
            "P/DCM-V":
            {
                "folder": "calculateData",
                "field":
                [
                    "metric_data","priceToValue"
                ],
                "isDateSeries": true,
                "condition": ">",
                "operator": "",
                "valueType": "float",
                "values":
                [
                    0.001
                ]
            }
        },
        "ranking":
        {
            "P/DCM-V":
            {
                "folder": "calculateData",
                "field":
                [
                    "metric_data","priceToValue"
                ],
                "isDateSeries": true,
                "measure": "recent",
                "direction": "smallestIsBest",
                "lowerBound": 0,
                "upperBound": null,
                "weight": 1.0,
                "plotSettings":
                {
                    "threshold": 0.75,
                    "lowerBound": 1E-7,
                    "upperBound": 10.0,
                    "logarithmic": false,
                    "width": 18.0,
                    "height": 4.5,
                    "addPlot": true
                }
            },
            "rCF/EV":
            {
                "folder": "calculateData",
                "field":
                [
                    "metric_data","residualCashFlowToEnterpriseValue"
                ],
                "isDateSeries": true,
                "measure": "recent",
                "direction": "biggestIsBest",
                "lowerBound": 0,
                "upperBound": null,
                "weight": 0.5,
                "plotSettings":
                {
                    "threshold": 0.05,
                    "lowerBound": -0.01,
                    "upperBound": 0.25,
                    "logarithmic": false,
                    "width": 18.0,
                    "height": 4.5,
                    "addPlot": true
                }
            },
            "ROIC-CC":
            {
                "folder": "calculateData",
                "field":
                [
                    "metric_data","returnOnInvestedCapitalLessCostOfCapital"
                ],
                "isDateSeries": true,
                "measure": "recent",
                "direction": "biggestIsBest",
                "lowerBound": 0,
                "upperBound": null,
                "weight": 0.5,
                "plotSettings":
                {
                    "threshold": 0.15,
                    "lowerBound": -0.01,
                    "upperBound": 0.25,
